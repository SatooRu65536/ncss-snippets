{
  "name": "ncss-snippets",
  "displayName": "ncss snippets",
  "description": "code snippets for ncss.",
  "version": "0.0.1",
  "publisher": "SatooRu65536",
  "icon": "icons/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SatooRu65536/ncss-snippets"
  },
  "bugs": {
    "url": "https://github.com/SatooRu65536/ncss-snippets/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "css",
    "ncss",
    "snippets"
  ],
  "scripts": {
    "v-patch": "npm version patch -m 'ðŸ”– Upgrade to %s'",
    "v-minor": "npm version minor -m 'ðŸ”– Upgrade to %s'",
    "v-major": "npm version major -m 'ðŸ”– Upgrade to %s'"
  },
  "contributes": {
    "languages": [
      {
        "id": "ncss",
        "extensions": [
          ".ncss"
        ]
      }
    ],
    "snippets": [
      {
        "language": "ncss",
        "path": "./snippets/command.code-snippets"
      },
      {
        "language": "ncss",
        "path": "./snippets/function.code-snippets"
      },
      {
        "language": "ncss",
        "path": "./snippets/syntax.code-snippets"
      }
    ]
  }
}